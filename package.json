{
  "name": "conqueror",
  "version": "1.0.0",
  "description": "Conqueror is a love letter to Dobyvatel that used to be. A game that was smothered in it's sleep.",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run start-back\" \"cd ./frontend && npm run dev\"",
    "start-back": "nodemon backend/server.js",
    "start-front": "cd ./frontend && npm run start",
    "post-start-front": "cd ..",
    "start": "concurrently \"npm run start-back\" \"npm run start-front\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
