    "game",
    "javascript",
    "socketio",
    "nextjs",
    "react"
  ],
  "author": "Patrik Dvořáček <patrik@dvoracek.dev> (https://dvoracek.dev)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tehSIRius/conqueror/issues"
  },
  "homepage": "https://github.com/tehSIRius/conqueror#readme",
  "dependencies": {
    "@material-ui/core": "^4.9.14",
    "@material-ui/icons": "^4.9.1",
    "babel-polyfill": "^6.26.0",
    "express": "^4.17.1",
    "history": "^4.10.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0"
