  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tehSIRius/conqueror/issues"
  },
  "homepage": "https://github.com/tehSIRius/conqueror#readme",
  "dependencies": {
    "bcrypt": "^4.0.1",
    "concurrently": "^5.1.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "helmet": "^3.22.0",
    "http": "0.0.1-security",
    "knex": "^0.21.0",
    "nodemon": "^2.0.3",
